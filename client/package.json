{
  "name": "mfe-client",
  "version": "1.1.1",
  "description": "AI Assistance",
  "scripts": {
    "build": "webpack --mode production && npm run types",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:start": "cd dist && ../node_modules/.bin/serve",
    "clean": "rm -rf node_modules dist __coverage__",
    "prepack": "npm run types",
    "start": "NODE_ENV=development node start-dev.js",
    "start:live": "NODE_ENV=development node start-dev.js --hot --live-reload",
    "test": "node --no-deprecation ./node_modules/.bin/jest",
    "test:ci": "npm run test -- --ci --coverage=false --bail",
    "types": "tsc src/types/index.ts --declaration --emitDeclarationOnly --outDir dist --skipLibCheck --lib es2022,dom,dom.iterable --target es2016 --moduleResolution node --esModuleInterop --resolveJsonModule --jsx react-jsx"
  },
  "eslintConfig": {
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn"
    }
  },
  "files": [
    "dist"
  ],
  "types": "dist/index",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.24.0",
    "@grapecity/wijmo.react.all": "^5.20241.19",
    "@core-components": "^2.12.0",
    "@core-styles": "^2.1.1",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.3.0",
    "html-webpack-plugin": "^5.6.0",
    "lodash": "^4.17.21",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-intersection-observer": "^9.8.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.21.1",
    "rehype-attr": "^3.0.3",
    "rehype-external-links": "^3.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^14.2.1",
    "serve": "^14.2.1",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.4.2",
    "webpack": "~5.101.0",
    "webpack-cli": "~6.0.1",
    "webpack-dev-middleware": "~7.4.2",
    "webpack-hot-middleware": "~2.26.1",
    "express": "~4.21.1",
    "http-proxy-middleware": "~2.0.7",
    "cors": "~2.8.5",
    "open": "~8.4.0",
    "zustand": "~4.5.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sass": "^1.70.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1"
  }
}
